{
    "cSpell.words": [
        "auxdir",
        "auxlock",
        "boldsymbol",
        "etoolbox",
        "ifstrempty",
        "ifstrequal",
        "latexmk",
        "lket",
        "midstick",
        "newcommand",
        "pdflatex",
        "qwbundle",
        "rket",
        "setwiretype",
        "sqrtt",
        "synctex",
        "texttt",
        "usepackage",
        "usetikzlibrary"
    ],
    "files.exclude": {
        "**/*.aux*": true,
        "*.blg": true,
        "*.fdb*": true,
        "*.fls": true,
        "*.log": true,
        "*.synctex*": true,
        "*.toc": true,
        "*.dpth": true,
        "tikz": true
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        }
    ]
}